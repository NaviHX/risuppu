(let ((greeting "Hello, what's your name?\n")
      (greeting-with-name '("Nice to meet you, " ".\n")))
  (do
    (-> (print greeting))
    (-> (read) (name))
    (-> (print (concat (car greeting-with-name) name (car (cdr greeting-with-name)))))
    name))

(define fact (lambda (n)
               (if (eq n 1) 1
                 (* n (fact (- n 1))))))

(let ((res (fact 10)))
  (do
    (-> (print "Fact 10 = "))
    (-> (print res))
    (-> (print " .\n"))))
